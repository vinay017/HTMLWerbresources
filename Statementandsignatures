    <script type="text/javascript">

        var sectionBarLabel = getDataParam();
        //get parent license  type


    if (sectionBarLabel)
    {
        var x = eval(sectionBarLabel);
        if (window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bo9_license"))
        var lookupItem = window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bo9_license").getValue()
        if (lookupItem == null)
        {
            if (window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bnr_applicantlicense"))
            lookupItem = window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bnr_applicantlicense").getValue()
        }
        if (lookupItem == null) {
            if (window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bo13e_applicantlicense"))
            lookupItem = window.parent.Xrm.Page.data.entity.attributes.get("dobnyc_bo13e_applicantlicense").getValue()
        }

        if (lookupItem != null) {
            var name = lookupItem[0].name;
            var guid = lookupItem[0].id;
            var entType = lookupItem[0].entityType;
            if (name.indexOf("BB")>-1) {
                var sectionBarLablTypeBB = getDataParam(); + "TypeBB"
                sectionBarLablTypeBB = sectionBarLablTypeBB + "TypeBB"
                x = eval(sectionBarLablTypeBB);
            }
        }

        if (x.style.display === 'none') {
            x.style.display = 'block';
        } else {
            x.style.display = 'none';
        }
    }
    </script>




